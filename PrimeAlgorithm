#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

void primality(int num)
{
        bool isPrime = true;
        int two = 2;
    
        if(num<two)
        {
            isPrime = false;            
        }
        else if(num == two)
        {
            isPrime = true;
        }
        else
        {
            int j=two; 
            int numTemp = num;
            while(j < numTemp)
            {
                if(num%j != 0)       
                {
                    numTemp = ceil(num/j);
                    j++;
                }
                else 
                {
                    isPrime = false;         
                    break;
                }
            }
        }
    
        if(isPrime)
            cout << "Prime" << endl;
        else
            cout << "Not prime" << endl;
        
        return;
}


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int T, n;
    cin >> T;
    vector <int> values;
    for(int i=0; i<T; i++)
    {
        cin >> n;
        values.push_back(n);  
        primality(n);
    }    
        
    return 0;
}
